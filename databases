CREATE TABLE `if18_kert_li_1`.`vpphotos` ( `id` INT(11) NOT NULL AUTO_INCREMENT , `userid` INT(11) NOT NULL , `filename` VARCHAR(40) NOT NULL , `created` DATE NOT NULL DEFAULT CURRENT_TIMESTAMP , `privacy` INT(1) NOT NULL , `deleted` TIMESTAMP NULL , `alttext` VARCHAR(256) NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;

